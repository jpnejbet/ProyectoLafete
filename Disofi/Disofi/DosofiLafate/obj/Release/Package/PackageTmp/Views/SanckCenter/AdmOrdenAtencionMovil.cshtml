

@{
    /**/

    ViewBag.Title = "Orden Atención";
}
<script src="~/WebJs/HomeJs/Usuario.js"></script>

<div class="right_col" role="main">

    <a id="Grabar" class="btn btn-app">  <i class="fa fa-save"></i> Grabar </a>


    <a class="btn btn-app" href="javascript:history.back()">  <i class="fa fa-mail-reply"></i> Volver </a>

    <br />


    <div class="x_content">
        <h3>ATENCIÓN SNACK CENTER</h3>
        <div class="row">
            <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">
                <h2>
                    Atención N°:
                    <input type="text" value="@ViewBag.Id" id="txtIdOt">
                </h2>

                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                    Seleccione Tipo de Atención  <span class="required">*</span>
                </label>
                <div class="col-md-2 col-sm-9 col-xs-12">
                    <input id="TipoAtencion" value="@ViewBag.TipoAtencion" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                </div>

            </form>
        </div>
    </div>


    <div class="x_content">
        <h3>DATO DEL CLIENTE</h3>
        <div class="row">
            <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">
                <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Seleccione Requerimiento <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <input id="Requemiento" value="@ViewBag.Requemiento" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>
                </div>
                <br />

                <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Seleccione Tipo Cliente<span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <input id="Requemiento" value="@ViewBag.TipoCliente" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>
                </div>
                <br />

                <div id="nombreCliente" class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Nombre Cliente <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <input id="NombreCliente" value="@ViewBag.NombreCliente" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>
                </div>




                <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Numero Contacto Cliente <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <input id="NumeroContacto" value="@ViewBag.NumeroContacto" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Descripción del Problema  <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <textarea class="form-control" rows="3" id="comment"> @ViewBag.Descripcion</textarea>
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Ubicación Fisica <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <input id="UbicacionFisica" value="@ViewBag.UbicacionFisica" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>
                </div>

            </form>
        </div>
    </div>



    <div class="x_content">
        <h3>ATENCIÓN MAQUINA </h3>
        <div class="row">
            <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">
                <div class="form-group">

                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Ruta
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <input id="Ruta" value="@ViewBag.Ruta" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>


                </div>


                <div class="form-group">

                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Maquina
                    </label>
                    <div class="col-md-2 col-sm-2 col-xs-12">
                        <input id="Maquina" value="@ViewBag.Maquina" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>

                    <label class="control-label col-md-1 col-sm-1 col-xs-12" for="first-name">
                        Modelo
                    </label>
                    <div class="col-md-2 col-sm-2 col-xs-12">
                        <input id="Modelo" value="@ViewBag.Modelo" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>

                    <label class="control-label col-md-1 col-sm-1 col-xs-12" for="first-name">
                        Tipo Maquina
                    </label>
                    <div class="col-md-2 col-sm-2 col-xs-12">
                        <input id="TipoMaquina" value="@ViewBag.TipoMaquina" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>

                </div>




                <div class="form-group">

                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Seleccione Diagnostico  <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <input id="Diganostico" value="@ViewBag.Diganostico" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">

                    </div>
                </div>

                <div class="form-group">

                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Seleccione Solución <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <input id="Diganostico" value="@ViewBag.Solucion" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">

                    </div>
                </div>


                <div class="form-group">

                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Seleccione Responsable  <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <select id="cmdResponsable" class="form-control">
                            <option></option>
                            <option>Francisco Martinez</option>
                            <option>Sebastian Gil</option>
                            <option>Jose Alcedo</option>
                            <option>Rubén araya</option>
                            <option>Magdalena Guajardo</option>
                            <option>Ana Salazar</option>
                            <option>Miguel Gutiérrez</option>
                            <option>David Retamal</option>
                            <option>Nelson Moreno</option>
                            <option>Jorge Jorquera</option>
                            <option>Danesa Moreira</option>
                            <option>Miguel Castillo</option>
                            <option>Freddy Peña</option>
                        </select>
                    </div>


                    <label class="control-label col-md-1 col-sm-1 col-xs-12" for="first-name">
                        Supervisor
                    </label>
                    <div class="col-md-2 col-sm-2 col-xs-12">
                        <input id="Supervisor" value="@ViewBag.Supervisor" class="form-control col-md-2 col-xs-12" type="text" name="middle-name" disabled="disabled">
                    </div>


                </div>
            </form>
        </div>
    </div>


    <div class="x_content">
        <h3>SOLUCION</h3>
        <div class="row">
            <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">

                <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Descripción del la solucución  <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <textarea class="form-control" rows="3" id="DescriSolucion"></textarea>
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                        Cerrar el Proceso  <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <button type="button" id="btnCerrarModal" class="btn btn-default" data-dismiss="modal">Cerrar Proceso</button>
                    </div>
                </div>


            </form>
        </div>
    </div>

</div>





