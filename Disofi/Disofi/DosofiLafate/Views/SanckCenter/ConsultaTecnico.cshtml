

@{
    /**/

    ViewBag.Title = "Consulta datos Tecnico";
}



<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css">

<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/buttons/1.5.2/css/buttons.dataTables.min.css">


<script src="~/WebJs/HomeJs/Agrup.js"></script>
<script src="~/WebJs/HomeJs/Usuario.js"></script>
<link href="~/Content/Agrup.css" rel="stylesheet" />

<div class="right_col" role="main">
    <h3>@ViewBag.Message</h3>

    <br />
    <a class="btn btn-app" href="@Url.Action("OrdenAtencion", "SanckCenter")">  <i class="fa fa-edit"></i> Nueva O.A. </a>

     


    <a class="btn btn-app" href="javascript:history.back()">  <i class="fa fa-mail-reply"></i> Volver </a>


    <br />



    <div class="x_panel">
        <div class="x_title">
            <h2>Consulta Ordenes Atención Técnica</h2>
            <ul class="nav navbar-right panel_toolbox">
                <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                </li>
                <li>
                    <a class="close-link"><i class="fa fa-close"></i></a>
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">
            <div class="row">

                <div class="x_content">


                    <div class="table-responsive">


                        @{
                            var grid = new WebGrid(Model, canPage: false);
                        }
                        @grid.GetHtml(
                                        htmlAttributes: new { id = "tblSeguimiento" },
                                        tableStyle: "display",
                                        columns:
                                        grid.Columns(
                                                     grid.Column(columnName: "Id", header: "N°", format: @<text>@item.Id</text>),
grid.Column(columnName: "Usuario", header: "Usuario", format: @<text>@item.Usuario</text>),
grid.Column(columnName: "FechaIngreso", header: "Fecha Ingreso", format: @<text>@item.FechaIngreso.ToString("dd/MM/yyyy")</text>),
grid.Column(columnName: "Requemiento", header: "Requerimiento", format: @<text>@item.Requemiento</text>),
grid.Column(columnName: "NombreCliente", header: "Nombr eCliente", format: @<text>@item.NombreCliente</text>),
grid.Column(columnName: "NumeroContacto", header: "N° Contacto", format: @<text> @item.NumeroContacto</text>),
grid.Column(columnName: "Responsable", header: "Responsable", format: @<text> @item.Responsable</text>),
grid.Column(columnName: "Estado", header: "Estado", format: @<text> @item.Estado</text>),
grid.Column(columnName: "Editar", header: "Editar", format: @<a class="btn btn-success" href="@Url.Action("AdmOrdenAtencion", "SanckCenter", new { id= @item.Id })">Editar</a>)
))


                    </div>
                </div>
            </div>

        </div>
    </div>
</div>




